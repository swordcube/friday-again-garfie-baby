{
    "atlas": {
		"type": "sparrow",
		"path": "sprite"
	},
    "offset": [0, -38],

    "scale": 1,
    "lineHeight": 60,
    
    "noLowerCase": true,
    "glyphs": {
        "'": {
            "offset": [0, 30]
        },
        "-": {
            "offset": [0, 20]
        },
        "=": {
            "offset": [0, 15]
        },
        "+": {
            "offset": [0, 15]
        },
        "^": {
            "offset": [0, 25]
        },
        "&": {
            "prefix": "and"
        },
        ",": {
            "prefix": "comma",
            "offset": [0, 8]
        },
        "~": {
            "offset": [0, 20]
        },
        "“": {
            "prefix": "start parentheses",
            "offset": [0, 30]
        },
        "”": {
            "prefix": "end parentheses",
            "offset": [0, 30]
        },
        "\"": {
            "prefix": "end parentheses",
            "offset": [0, 30]
        },
        "!": {
            "prefix": "exclamation mark"
        },
        "#": {
            "prefix": "hashtag"
        },
        "$": {
            "prefix": "dollar"
        },
        "%": {
            "prefix": "percent"
        },
        ":": {
            "prefix": "colon"
        },
        ";": {
            "prefix": "semicolon"
        },
        "\\": {
            "prefix": "backslash"
        },
        "/": {
            "prefix": "forward slash"
        },
        " ": {
            "width": 34,
            "height": 34,
            "visible": false
        },
        "\t": {
            "width": 74,
            "height": 34,
            "visible": false
        }
    }
}